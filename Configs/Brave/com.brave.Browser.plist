<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

    <!-- ========================== -->
    <!-- BRAVE FEATURES             -->
    <!-- ========================== -->
    <key>BraveNewsEnabled</key>
    <false/>
    <key>RewardsEnabled</key>
    <false/>
    <key>TalkEnabled</key>
    <false/>
    <key>VPNEnabled</key>
    <false/>

    <!-- ========================== -->
    <!-- SHIELDS                    -->
    <!-- ========================== -->
    <key>TrackerBlockingAggressive</key>
    <true/>
    <key>HttpsOnlyMode</key>
    <string>2</string>
    <key>PreventFingerprintingBasedOnLanguage</key>
    <true/>
    <key>BlockThirdPartyCookies</key>
    <true/>
    <key>StoreContactInfoForBrokenSiteReports</key>
    <false/>
    <key>SocialMediaBlockingFacebook</key>
    <false/>
    <key>SocialMediaBlockingLinkedIn</key>
    <false/>
    <key>SocialMediaBlockingX</key>
    <false/>

    <!-- ========================== -->
    <!-- PRIVACY & SECURITY         -->
    <!-- ========================== -->
    <key>SafeBrowsingMode</key>
    <string>standard</string>
    <key>DnsOverHttpsMode</key>
    <string>secure</string>
    <key>DnsOverHttpsTemplates</key>
    <string>https://dns.controld.com/10eulxnerys</string>
    <key>ClearUnusedSitePermissions</key>
    <true/>
    <key>WebRTCIPHandlingPolicy</key>
    <string>disable_non_proxied_udp</string>
    <key>UseGoogleServicesForPushMessaging</key>
    <false/>
    <key>AutoRedirectAMPPages</key>
    <true/>
    <key>AutoRedirectTrackingURLs</key>
    <true/>
    <key>PreventSitesFingerprintingMe</key>
    <true/>
    <key>SendDoNotTrack</key>
    <true/>
    <key>TorDisabled</key>
    <true/>
    <key>OnlyResolveOnionInTorWindows</key>
    <false/>
    <key>VolunteerToHelpTor</key>
    <false/>
    <key>UseTorBridges</key>
    <false/>
    <key>AllowPrivacyPreservingProductAnalytics</key>
    <false/>
    <key>SendDailyUsagePing</key>
    <false/>
    <key>SendDiagnosticReports</key>
    <false/>
    <key>BraveSurveysEnabled</key>
    <false/>

    <!-- ========================== -->
    <!-- SEARCH ENGINES             -->
    <!-- ========================== -->
    <!-- Normal Window: DuckDuckGo SafeSearch -->
    <key>DefaultSearchProviderName</key>
    <string>DuckDuckGo Safe</string>
    <key>DefaultSearchProviderSearchURL</key>
    <string>https://duckduckgo.com/?q=%s&kp=1</string>

    <!-- Private Window: Brave SafeSearch -->
    <key>PrivateWindowSearchProviderName</key>
    <string>Brave Safe</string>
    <key>PrivateWindowSearchProviderSearchURL</key>
    <string>https://safe.search.brave.com/search?q=%s</string>

    <key>ImproveSearchSuggestionsEnabled</key>
    <false/>
    <key>WebDiscoveryProjectEnabled</key>
    <false/>
    <key>SearchEnginesIncludeOtherEngines</key>
    <false/>

    <!-- ========================== -->
    <!-- EXTENSIONS                 -->
    <!-- ========================== -->
    <key>AllowGoogleLoginForExtensions</key>
    <false/>
    <key>MediaRouterEnabled</key>
    <true/>
    <key>WidevineEnabled</key>
    <true/>
    <key>ManifestV2Enabled</key>
    <false/>

    <!-- Force Install Extensions -->
    <key>ExtensionInstallForceList</key>
    <array>
        <string>bgnkhhnnamicmpeenaelnjfhikgbkllg;https://clients2.google.com/service/update2/crx</string>
        <string>lfkekhbpfmnclpodkoohbnhniddepehk;https://clients2.google.com/service/update2/crx</string>
        <string>abdkjmofmjelgafcdffaimhgdgpagmop;https://clients2.google.com/service/update2/crx</string>
        <string>pncfbmialoiaghdehhbnbhkkgmjanfhe;https://clients2.google.com/service/update2/crx</string>
    </array>

    <!-- ========================== -->
    <!-- AUTOFILL & PASSWORDS       -->
    <!-- ========================== -->
    <key>OfferToSavePasswordsAndPasskeys</key>
    <false/>
    <key>SignInAutomatically</key>
    <false/>
    <key>SaveAndFillPaymentMethods</key>
    <false/>
    <key>VerifyBeforeAutofillPaymentMethods</key>
    <false/>
    <key>SaveSecurityCodes</key>
    <false/>
    <key>AllowSitesToCheckSavedPaymentMethods</key>
    <false/>
    <key>SaveAndFillAddresses</key>
    <false/>

    <!-- ========================== -->
    <!-- LANGUAGES                  -->
    <!-- ========================== -->
    <key>CheckSpellingEnabled</key>
    <false/>

</dict>
</plist>